				<div class="card">
					<!--begin::Card header-->
					<div class="card-header border-0 pt-6">
						<!--begin::Card title-->
						<div class="card-title">
							<!--begin::Search-->
							<div class="d-flex align-items-center position-relative my-1">
								<!--begin::Svg Icon | path: icons/duotune/general/gen021.svg-->
								<span class="svg-icon svg-icon-1 position-absolute ms-6">
									<svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
										<rect opacity="0.5" x="17.0365" y="15.1223" width="8.15546" height="2" rx="1" transform="rotate(45 17.0365 15.1223)" fill="currentColor" />
										<path d="M11 19C6.55556 19 3 15.4444 3 11C3 6.55556 6.55556 3 11 3C15.4444 3 19 6.55556 19 11C19 15.4444 15.4444 19 11 19ZM11 5C7.53333 5 5 7.53333 5 11C5 14.4667 7.53333 17 11 17C14.4667 17 17 14.4667 17 11C17 7.53333 14.4667 5 11 5Z" fill="currentColor" />
									</svg>
								</span>
								<!--end::Svg Icon-->
                                <input id="SearchPatientWorklist" type="text" data-kt-customer-table-filter="search" class="form-control form-control-solid w-250px ps-15" placeholder="Search " />
							</div>
							<!--end::Search-->
						</div>
						<!--begin::Card title-->
						<!--begin::Card toolbar-->
						<div class="card-toolbar">
							<!--begin::Toolbar-->
							<div class="d-flex justify-content-end" data-kt-customer-table-toolbar="base">
								<!--begin::Add customer-->
								<button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#kt_modal_add_customer">Add Patient</button>
								<!--end::Add customer-->
							</div>
							<!--end::Toolbar-->

						</div>
						<!--end::Card toolbar-->
					</div>
					<!--end::Card header-->
					<!--begin::Card body-->
					<div class="card-body pt-0">
						<!--begin::Table-->
@*                         id="kt_customers_table"
 *@                        <table class="table align-middle table-row-dashed fs-6 gy-5"  id="displayReportColumns">
							<!--begin::Table head-->
                            <thead class="border-bottom border-gray-200 fs-7 fw-bold">
								<!--begin::Table row-->
								<tr class="text-start text-gray-400 fw-bold fs-7 text-uppercase gs-0">

									<th class="min-w-125px sorting text-dark " tabindex="0" aria-controls="kt_customers_table" rowspan="1" colspan="1" aria-label="Customer Name: activate to sort column ascending">Name</th>
									<th class="min-w-125px sorting text-dark " tabindex="0" aria-controls="kt_customers_table" rowspan="1" colspan="1" aria-label="Customer Name: activate to sort column ascending">Address</th>
									<th class="min-w-125px sorting text-dark " tabindex="0" aria-controls="kt_customers_table" rowspan="1" colspan="1" aria-label="Customer Name: activate to sort column ascending">Email</th>
									<th class="min-w-125px sorting text-dark " tabindex="0" aria-controls="kt_customers_table" rowspan="1" colspan="1" aria-label="Customer Name: activate to sort column ascending">Gender</th>
									<th class="min-w-125px sorting text-dark " tabindex="0" aria-controls="kt_customers_table" rowspan="1" colspan="1" aria-label="Customer Name: activate to sort column ascending">BirthDate</th>
									<th class="min-w-125px sorting text-dark " tabindex="0" aria-controls="kt_customers_table" rowspan="1" colspan="1" aria-label="Customer Name: activate to sort column ascending">Phone</th>
									<th class="text-center min-w-70px">Actions</th>
								</tr>
								<!--end::Table row-->
							</thead>
							<!--end::Table head-->
							<!--begin::Table body-->
							<tbody class="fw-semibold text-gray-600">
								
									@foreach (var item in Model)
									{
									    <tr>
									    	<!--begin::Name=-->
									    	<td>
									    		<a asp-action="Details" asp-controller="patient" asp-route-id="@item.PatientId" class="text-gray-800 text-hover-primary mb-1">@item.PatientName</a>
									    	</td>
									    	<!--end::Name=-->
									    	<!--begin::Address=-->
									    	<td>
									    		<span class="text-gray-600 mb-1">@item.PatientAddress</span>
									    	</td>
									    	<!--end::Address=-->
									    	<!--begin::Email=-->
									    	<td>
									    		<span class="text-gray-600  mb-1">@item.PatientEmail</span>
									    	</td>
									    	<!--end::Email=-->
									    	<!--begin::Gender=-->
									    	<td>
									    		<span class="text-gray-600  mb-1">@item.PatientGender</span>
									    	</td>
									    	<!--end::Gender=-->
									    	<!--begin::BirthDate-->
									    	<td >
                                            @if (item.PatientBirthdate != null)
                                            {
                                                <span class="text-gray-600  mb-1" id="birthdate">@item.PatientBirthdate.Value.ToString("yyyy-MM-dd")</span>
                                            }
                                        </td>
									    	<!--end::BirthDate-->
									    	<!--begin::Phone=-->
									    	<td>
									    		<span class="text-gray-600  mb-1">@item.PatientTel1</span>	
									    	</td>
									    	<!--end::Phone=-->
									    	<!--begin::Action=-->
									    	<td class="text-end">
									    		<div class="btn btn-sm btn-light btn-active-light-primary"data-kt-menu-trigger="click" data-kt-menu-placement="bottom-end" >
									    			Actions
									    			<!--begin::Svg Icon | path: icons/duotune/arrows/arr072.svg-->
									    			<span class="svg-icon svg-icon-5 m-0">
									    				<svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
									    					<path d="M11.4343 12.7344L7.25 8.55005C6.83579 8.13583 6.16421 8.13584 5.75 8.55005C5.33579 8.96426 5.33579 9.63583 5.75 10.05L11.2929 15.5929C11.6834 15.9835 12.3166 15.9835 12.7071 15.5929L18.25 10.05C18.6642 9.63584 18.6642 8.96426 18.25 8.55005C17.8358 8.13584 17.1642 8.13584 16.75 8.55005L12.5657 12.7344C12.2533 13.0468 11.7467 13.0468 11.4343 12.7344Z" fill="currentColor" />
									    				</svg>
									    			</span>
									    			<!--end::Svg Icon-->
									    		</div>
