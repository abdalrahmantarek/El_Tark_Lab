	// Function to export data to Excel
	function exportToExcel() {
		// Create a new workbook
		var workbook = new ExcelJS.Workbook();
		var worksheet = workbook.addWorksheet('Sheet1');

		// Add header row
		var header = [];
		$("#displaySevice thead th").each(function () {
			header.push($(this).text().trim()); // Trim the header text
		});
		worksheet.addRow(header);

		// Add data rows
		table.rows().nodes().each(function (row) {
			var rowData = [];
			$(row).find(".flex-grow-1 a, .flex-grow-1 span").each(function () {
				rowData.push($(this).text().trim());
			});
			worksheet.addRow(rowData);
		});

		// Create a blob for the workbook
		workbook.xlsx.writeBuffer().then(function (buffer) {
			// Create a Blob object and create a URL to trigger the download
			var blob = new Blob([buffer], { type: 'application/vnd.openxmlformats-officedocument.spreadsheetml.sheet' });
			var url = window.URL.createObjectURL(blob);

			// Create a link and trigger the download
			var a = document.createElement('a');
			a.href = url;
			a.download = 'ExportedData.xlsx';
			a.click();

			// Clean up
			window.URL.revokeObjectURL(url);
		});
	}

	function loadPartial(url, divID) {

		$('#' + divID).html("");
		$('#' + divID).append("<div class='spinner-border text-primary' style='display: block; margin-left: auto; margin-right: auto;' role='status'>" +
			"<span class='visually-hidden'>Loading...</span>" +
			"</div>");

		$('#' + divID).load(url);
	}
	</script>
